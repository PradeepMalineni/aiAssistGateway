# Enterprise Services
DOCUMENT_SEARCH_SERVICE_URL=https://document-search.wellsfargo.com
POLICY_CATALOG_SERVICE_URL=https://policy-catalog.wellsfargo.com
AUDIT_SERVICE_URL=https://audit.wellsfargo.com

# Security
ENCRYPTION_KEY=${ENTERPRISE_ENCRYPTION_KEY}
JWT_SECRET=${ENTERPRISE_JWT_SECRET}
